pipeline {
    agent any
    parameters {
        file(name: 'UPLOAD_FILE', description: 'Select a file to upload')
    }
    stages {
        stage('Print File Contents') {
            steps {
                script {
                    // Check if the file parameter is provided
                    if (params.UPLOAD_FILE) {
                        // Read and print the file contents
                        def fileContent = readFile file: params.UPLOAD_FILE
                        echo "File Contents:\n${fileContent}"
                    } else {
                        echo "No file uploaded."
                    }
                }
            }
        }
    }
}
